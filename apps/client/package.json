{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "lint:all": "run-p run-tsc run-eslint run-stylelint",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "run-tsc": "tsc",
    "run-eslint": "eslint \"src/**/*.ts\" --cache --fix --ignore-path .gitignore",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore **/*.css"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@tabler/icons-react": "^2.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-router-dom": "6.11.2",
    "sonner": "^0.4.0"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "1.5.0",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/css-mediaquery": "0.1.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-router-dom": "5.3.3",
    "@types/testing-library__jest-dom": "5.14.5",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-istanbul": "0.31.1",
    "autoprefixer": "10.4.14",
    "css-mediaquery": "0.1.2",
    "eslint": "8.41.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-testing-library": "5.11.0",
    "jsdom": "22.0.0",
    "msw": "1.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "stylelint": "15.6.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "33.0.0",
    "supertest": "^6.3.3",
    "tailwindcss": "3.3.2",
    "ts-standard": "^12.0.2",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.1"
  },
  "browserslist": {
    "production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}